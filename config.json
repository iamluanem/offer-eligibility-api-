{
  "server": {
    "port": "3000",
    "host": "",
    "enable_tls": false
  },
  "database": {
    "path": "./custom_path.db"
  },
  "security": {
    "max_request_body_size": 10485760,
    "allowed_origins": "*"
  },
  "rate_limit": {
    "enabled": true,
    "rate": 100,
    "window": 60
  },
  "tracing": {
    "enabled": false
  },
  "features": {
    "cache_enabled": false,
    "event_hooks_enabled": false
  },
  "cache": {
    "enabled": false
  }
}
